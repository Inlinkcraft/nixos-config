(defwindow clock [width height]
    :monitor 0
    :geometry (geometry 
        :x "${0.335 * width}px" 
        :y "${0.25 * height}px" 
        :width "${0.33 * width}px" 
        :height "${0.25 * height}px" 
        :anchor "top left"
    )
    :stacking "overlay"
    :exclusive false
    :focusable "ondemand"
    (box
        :orientation "h"
        "03"
        "|"
        "51"
    )
)

