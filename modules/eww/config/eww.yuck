(defwindow clock [width height]
    :monitor 0
    :geometry (geometry 
        :x "500px" 
        :y "500px" 
        :width "500px" 
        :height "$500px" 
        :anchor "center"
    )
    :stacking "overlay"
    :exclusive false
    :focusable "ondemand"
    (box
        :orientation "h"
        "03"
        "|"
        "51"
    )
)

